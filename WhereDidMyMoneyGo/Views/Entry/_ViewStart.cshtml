@{
    /*
    This calls a custom layout for the Entry Screen.

    Before each View is accessed after being called by controller,
    the _ViewStart file within the root of View's folder will take priority
    over the default _ViewStart file in the Views folder.

    This specific file is assigning the layout to be _EntryLayout
    versus the default _Layout.
    */

    //Layout = "_Layout";
    Layout = "~/Views/Shared/_EntryLayout.cshtml";
}
